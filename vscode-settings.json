{
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "file-icons",
  "files.autoSave": "onFocusChange",
  "editor.minimap.enabled": false,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "workbench.sideBar.location": "right",
  "editor.fontFamily": "Fira Code, CascadiaCode Nerd Font, Cascadia Code, 'Courier New', monospace",
  "window.menuBarVisibility": "toggle",
  "editor.fontLigatures": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "typescript.validate.enable": true,
  "javascript.validate.enable": true,
  "zenMode.hideTabs": false,
  "editor.suggestSelection": "first",
  "editor.occurrencesHighlight": false,
  "editor.lineNumbers": "relative",
  /*
   * Color settings
   */
  "workbench.colorTheme": "Ocean Dark Extended",
  "terminal.integrated.fontWeightBold": "500",
  "workbench.colorCustomizations": {
    "list.focusBackground": "#1D2027",
    "editor.background": "#1D2027",
    "editorGroupHeader.tabsBackground": "#1D2027",
    "tab.activeBorder": "#c0c5ce",
    "terminal.background": "#2B303B",
    "terminal.foreground": "#C0C5CE",
    "terminalCursor.background": "#C0C5CE",
    "terminalCursor.foreground": "#C0C5CE",
    "terminal.ansiBlack": "#2B303B",
    "terminal.ansiBlue": "#8FA1B3",
    "terminal.ansiBrightBlack": "#65737E",
    "terminal.ansiBrightBlue": "#8FA1B3",
    "terminal.ansiBrightCyan": "#96B5B4",
    "terminal.ansiBrightGreen": "#A3BE8C",
    "terminal.ansiBrightMagenta": "#B48EAD",
    "terminal.ansiBrightRed": "#BF616A",
    "terminal.ansiBrightWhite": "#EFF1F5",
    "terminal.ansiBrightYellow": "#EBCB8B",
    "terminal.ansiCyan": "#96B5B4",
    "terminal.ansiGreen": "#A3BE8C",
    "terminal.ansiMagenta": "#B48EAD",
    "terminal.ansiRed": "#BF616A",
    "terminal.ansiWhite": "#C0C5CE",
    "terminal.ansiYellow": "#EBCB8B"
  },
  "bracket-pair-colorizer-2.colors": ["#ebcb8b", "#b48ead", "#8fa1b3"],
  /*
   * Vsvim bindings
   */
  "vim.surround": true,
  "vim.sneak": true,
  "vim.leader": "space",
  "vim.visualstar": true,
  "vim.camelCaseMotion.enable": true,
  "vim.replaceWithRegister": true,
  "vim.useCtrlKeys": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["k", "j"],
      "commands": [
        "extension.vim_escape",
        "editor.action.formatDocument",
        "workbench.action.files.save"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    }
  ],
  "vim.normalModeKeyBindings": [
    // jump to function signature
    {
      "before": ["<leader>", "s"],
      "after": ["[", "{", "[", "{", "[", "{", "[", "{", "[", "{"]
    },
    // jump to function signature and show references
    {
      "before": ["<leader>", "r"],
      "after": [
        "g",
        "g",
        "/",
        "e",
        "x",
        "p",
        "o",
        "r",
        "t",
        " ",
        "c",
        "o",
        "n",
        "s",
        "t",
        "<enter>",
        "w",
        "w",
        "<leader>",
        "g",
        "r"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "g", "r"],
      "commands": [
        { "command": ":nohl" },
        { "command": "editor.action.goToReferences" }
      ]
    },
    { "before": [":"], "after": [","] },
    { "before": [","], "after": [":"] },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["K"],
      "after": ["i", "<enter>", "<esc>"]
    },
    // centres after jumping to next
    {
      "before": ["n"],
      "after": ["n", "z", "z", "z", "o"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z", "z", "o"]
    },
    // attributes
    {
      "before": ["<leader>", "d", "a"],
      "after": ["v", "i", "<", "o", "W", "h", "d", "<esc>", "<leader>", "h"]
    },
    // add comma to eol
    {
      "before": ["<leader>", "a", "c"],
      "after": ["A", ",", "<esc>", "j", "^"]
    },
    // add property
    {
      "before": ["<leader>", "a", "p"],
      "after": ["A", ",", "<enter>"]
    },
    // delete property
    {
      "before": ["<leader>", "d", "p"],
      "after": ["V", "$", "%", "d"]
    },
    // add space
    {
      "before": ["<leader>", "a", "s"],
      "after": ["a", " ", "<esc>"]
    },
    {
      "before": ["<leader>", "%"],
      "commands": [
        {
          "command": "editor.emmet.action.matchTag"
        }
      ]
    },
    // find and change inside <char>
    {
      "before": ["<leader>", "`"],
      "after": ["f", "`", "c", "i", "`"]
    },
    {
      "before": ["<leader>", "\""],
      "after": ["f", "\"", "c", "i", "\""]
    },
    {
      "before": ["<leader>", "'"],
      "after": ["f", "'", "c", "i", "'"]
    },
    {
      "before": ["<leader>", ")"],
      "after": ["f", ")", "c", "i", ")"]
    },
    {
      "before": ["<leader>", "("],
      "after": ["f", "(", "c", "i", "("]
    },
    {
      "before": ["<leader>", "{"],
      "after": ["f", "{", "c", "i", "{"]
    },
    {
      "before": ["<leader>", "}"],
      "after": ["f", "}", "c", "i", "}"]
    },
    {
      "before": ["<leader>", "["],
      "after": ["f", "[", "c", "i", "["]
    },
    {
      "before": ["<leader>", "]"],
      "after": ["f", "]", "c", "i", "]"]
    },
    // paste 0 buffer - last yank
    {
      "before": ["<leader>", "'", "p"],
      "after": ["\"", "0", "p"]
    },
    {
      "before": ["<leader>", "'", "P"],
      "after": ["\"", "0", "P"]
    },
    // paste line break and then buffer
    {
      "before": ["<leader>", "p", "l"],
      "after": ["o", "<esc>", "p"]
    },
    // paste end - add spaces then paste buffer from end of word
    {
      "before": ["<leader>", "p", "e"],
      "after": ["E", "a", " ", "<esc>", "v", "p"]
    },
    // paste property (object)
    {
      "before": ["<leader>", "p", "p"],
      "after": ["A", ",", "<esc>", "p"]
    },
    {
      "before": ["<leader>", "f"],
      "after": ["/"]
    },
    {
      "before": ["<leader>", "F"],
      "after": ["?"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "t", "o"],
      "commands": [":tabonly"]
    },
    // hack to jump over folds
    {
      "before": ["<C-u>"],
      "after": ["H", "z", "z"]
    },
    {
      "before": ["<C-d>"],
      "after": ["L", "z", "z"]
    }
  ],
  "cSpell.userWords": [
    "aboutme",
    "collapsable",
    "containerregistry",
    "datacapture",
    "ECHAL",
    "EEFGR",
    "Eitel",
    "Errored",
    "firestore",
    "gameplay",
    "gcloud",
    "googleapis",
    "gserviceaccount",
    "Microservices",
    "nowrap",
    "Parens",
    "Pathing",
    "rebass",
    "receiptuploader",
    "scroller",
    "secretcodes",
    "SENDGRID",
    "Sidedrawer",
    "Sidesheet",
    "Spotify",
    "Subsea",
    "Treeview",
    "Unticked",
    "unvalidated",
    "Xunit"
  ],
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "html.validate.scripts": false,
  "scm.alwaysShowRepositories": true,
  "window.title": "",
  "files.exclude": {
    "bin": true,
    "obj": true,
    "out": true
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 8,
  "workbench.statusBar.visible": true,
  "editor.renderControlCharacters": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[csharp]": { "editor.defaultFormatter": "ms-dotnettools.csharp" },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "settingsSync.ignoredExtensions": ["dbaeumer.vscode-eslint"],
  "diffEditor.renderSideBySide": false,
  "redhat.telemetry.enabled": false,
  "workbench.editor.wrapTabs": true,
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "debug.javascript.terminalOptions": {
    "skipFiles": ["<node_internals>/**", "**.js"]
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.enabled": false,
  "debug.javascript.suggestPrettyPrinting": false,
  "editor.renderWhitespace": "none",
  "workbench.activityBar.visible": false
}
